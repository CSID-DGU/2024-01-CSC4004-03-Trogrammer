<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFFFF"
	android:clipChildren="false"
	android:clipToPadding="false"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView 
		android:fillViewport="true"
		android:id="@+id/rpq6t58ev2m"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_weight="1"
		android:background="@drawable/fm_sff000000sw1bfff6f6f6"
		android:clipChildren="false"
		android:clipToPadding="false">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:clipChildren="false"
			android:clipToPadding="false"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/rqtmkruvho2"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#FF1F1F1F"
				android:paddingTop="125dp"
				android:clipChildren="false"
				android:clipToPadding="false"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/rnrn5spr0gl"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					android:background="@drawable/fm_cr10bffffffff"
					android:paddingVertical="13dp"
					android:paddingHorizontal="20dp"
					android:layout_marginBottom="32dp"
					android:layout_marginHorizontal="39dp"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/rr12p6xmye4p"
						android:textColor="#000000"
						android:textSize="20sp"
						android:layout_marginEnd="4dp"
						android:layout_height="wrap_content"
						android:layout_width="match_parent"
						android:layout_weight="1"
						android:text="음식을 선택하세요"
					/>
					<ImageView
						android:id="@+id/rli373h46v2j"
						android:layout_width="24dp"
						android:layout_height="24dp"
						android:src="@drawable/SearchBar"
						android:scaleType="fitXY"
					/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/rg1adxn70gsa"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="@drawable/fm_cr20bfff6f6f6"
					android:paddingHorizontal="68dp"
					android:clipChildren="false"
					android:clipToPadding="false"
					android:orientation="vertical">
					<RelativeLayout
						android:id="@+id/r9f599op7nh5"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="52dp"
						android:clipChildren="false"
						android:clipToPadding="false"
						>
						<LinearLayout
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:clipChildren="false"
							android:clipToPadding="false"
							android:orientation="vertical">

							<TextView
								android:id="@+id/rq258hn3hulh"
								android:layout_width="290dp"
								android:layout_height="wrap_content"
								android:text="현재 냉장고에 있는 식재료"
								android:textColor="#000000"
								android:textSize="24sp" />

							<ImageView
								android:id="@+id/rd9o0pwo3gnv"
								android:layout_width="300dp"
								android:layout_height="350dp"
								android:layout_marginEnd="-50dp"
								android:layout_marginBottom="-385dp"
								android:scaleType="fitXY"
								android:src="@drawable/Refrigerator" />
						</LinearLayout>

					</RelativeLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
